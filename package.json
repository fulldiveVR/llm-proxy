{
  "name": "aiwize-llm-proxy",
  "version": "1.0.0",
  "description": "AIWIZE LLM Proxy service",
  "scripts": {
    "build": "rm -rf dist/* && nest build",
    "build:tests": "rm -rf dist/* && nest build ./tsconfig.tests.json",
    "start": "yarn start:dev",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production && node dist/main",
    "test": "yarn lint && yarn build && yarn test:unit && yarn test:int",
    "test:unit": "NODE_ENV=test mocha 'tests/unit/**/*.ts'",
    "test:int": "NODE_ENV=test mocha 'tests/integration/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "prettier-format": "prettier --config .prettierrc.yml 'src/**/*.ts' --write",
    "generate:domain:core": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=useReadonlyTypes=true --ts_proto_opt=env=node --ts_proto_opt=outputEncodeMethods=false --ts_proto_opt=outputJsonMethods=false --ts_proto_opt=unrecognizedEnum=false --ts_proto_opt=stringEnums=true --experimental_allow_proto3_optional --ts_proto_opt=removeEnumPrefix=true",
    "generate:domain:auth": "yarn generate:domain:core --ts_proto_out ./src/domain/_generated/auth --proto_path ./domain/ai-auth/v1/ ./domain/ai-auth/v1/*.proto",
    "generate:domain:billing": "yarn generate:domain:core --ts_proto_out ./src/domain/_generated/billing --ts_proto_opt=stringEnums=false --proto_path ./domain/billing/v1/ ./domain/billing/v1/*.proto",
    "generate:domain": "yarn generate:domain:auth && yarn generate:domain:billing"
  },
  "main": "src/index.ts",
  "author": "Fulldivevr Team",
  "license": "Fulldivevr team license",
  "private": true,
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.7.0",
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google-vertex": "^2.2.24",
    "@ai-sdk/openai": "^1.3.22",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/common": "^10.2.4",
    "@nestjs/core": "^10.2.4",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.2.4",
    "@nestjs/swagger": "^7.1.10",
    "ai": "^4.3.16",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "langfuse": "^3.37.4",
    "mocha": "^10.2.0",
    "mongodb": "^5.7.0",
    "openai": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "zod": "^3.25.57"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.4",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/mailgun-js": "^0.22.13",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/rss": "^0.0.30",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chai": "^4.3.7",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-local": "^1.0.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.156.5",
    "typescript": "^5.0.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
